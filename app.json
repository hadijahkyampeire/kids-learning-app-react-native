{
  "expo": {
    "name": "kids-learning-app-react-native",
    "slug": "kids-learning-app-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ecfccb",
      "dark": {
        "image": "./src/assets/splash-icon.png",
        "backgroundColor": "#0f172a"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourco.kidslearning"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.yourco.kidslearning",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen",
      "expo-font"
    ]
  }
}
